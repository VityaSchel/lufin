{
  "compilerOptions": {
    "module": "es2022",
    "target": "es2022",
    "lib": [
      "es2022"
    ],
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "useUnknownInCatchVariables": false,
    "declaration": true
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "test",
    "out"
  ]
}